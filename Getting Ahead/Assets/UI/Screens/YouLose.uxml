    <ui:VisualElement name="VisualElement" style="flex-grow: 1; position: absolute; bottom: 0; width: 100%; height: 100%; background-color: rgba(0, 0, 0, 0);">
        <ui:VisualElement name="MenuLogo" class="video-player" style="flex-grow: 0; background-image: url(&apos;project://database/Assets/UI/Main%20Menu%20Renders/Main%20Menu%20Logo.renderTexture?fileID=8400000&amp;guid=8a3f5c39372762442b9f4695bbff4c15&amp;type=2#Main Menu Logo&apos;); height: 100%; background-color: rgb(0, 0, 0);" />
    </ui:VisualElement>
    <ui:VisualElement name="MainMenu" style="flex-grow: 1; flex-direction: row; background-color: rgba(0, 0, 0, 0);">
        <ui:VisualElement name="LeftScreen" style="flex-grow: 1; width: 30%;" />
        <ui:VisualElement name="MiddleScreen" style="flex-grow: 1; flex-direction: column; flex-shrink: 1;">
            <ui:VisualElement name="TopGap" style="flex-grow: 1; height: 15%; flex-shrink: 1;" />
