                <ui:Label tabindex="-1" text="GAME OVER&#10;Next time, try using your head!" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Description" style="font-size: 100%; color: rgb(255, 255, 255); -unity-font-style: normal; -unity-text-align: upper-center; align-self: center;" />
            </ui:VisualElement>
            <ui:VisualElement name="ButtonSpace" style="flex-grow: 0; width: 70%; height: 100%; align-self: center; flex-direction: column;">
